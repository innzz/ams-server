{ 
    "version": 2, 
    "builds": [{"src": "./index.ts", "use": "@vercel/node"}],
    "rewrites": [
        { "source": "/(.*)", "destination": "./index.ts" }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/https://ams-server-5ctnmr1lc-innzzs-projects.vercel.app/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }  
    ]
}
